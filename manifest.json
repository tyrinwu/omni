{   
     "name": "DictAsYouGo",
    "version": "1.0",
    "manifest_version": 2,
    "description": "A dictionary that stores and remind you the words you search.",
    "browser_action": {
        "default_icon": "icon.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "permissions": [
        "tabs"
    ]
}

